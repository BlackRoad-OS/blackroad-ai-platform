    <script>
        // Handle /app route redirect to prevent loop (needs to run immediately)
        if (window.location.pathname === '/app' || window.location.pathname === '/app/') {
            window.history.replaceState({}, '', '/');
        }

        // Wait for DOM to be ready before attaching event listeners
        document.addEventListener('DOMContentLoaded', function() {

            // ========== MAIN TAB NAVIGATION ==========
            document.querySelectorAll('.main-tab').forEach(tab => {
                tab.addEventListener('click', () => {
                    const targetTab = tab.dataset.tab;

                    // Update tab buttons
                    document.querySelectorAll('.main-tab').forEach(t => {
                        t.classList.remove('active');
                        t.setAttribute('aria-selected', 'false');
                    });
                    tab.classList.add('active');
                    tab.setAttribute('aria-selected', 'true');

                    // Update panels
                    document.querySelectorAll('.tab-panel').forEach(panel => {
                        panel.classList.remove('active');
                    });
                    document.getElementById(`${targetTab}-panel`).classList.add('active');
                });

                // Keyboard navigation
                tab.addEventListener('keydown', (e) => {
                    if (e.key === 'Enter' || e.key === ' ') {
                        e.preventDefault();
                        tab.click();
                    }
                });
            });

            // ========== MEMORY SECTION - REAL API ==========

            // Load memory stats on tab switch
            async function loadMemoryStats() {
                try {
                    const response = await fetch(`${API_BASE}/api/memory/stats`);
                    const data = await response.json();

                    document.getElementById('journal-count').textContent = data.journalEntries.toLocaleString();
                    document.getElementById('context-links').textContent = data.codexComponents.toLocaleString();
                    document.getElementById('sync-latency').textContent = data.syncLatency;
                    document.getElementById('active-contexts').textContent = data.activeContexts;
                } catch (e) {
                    console.log('Memory stats: using defaults');
                }
            }

            // Load recent memories
            async function loadRecentMemories() {
                try {
                    const response = await fetch(`${API_BASE}/api/memory/recent?limit=10`);
                    const data = await response.json();

                    if (data.entries && data.entries.length > 0) {
                        const timeline = document.getElementById('memory-timeline');
                        timeline.innerHTML = data.entries.map(entry => `
                            <div class="memory-entry">
                                <div class="memory-time">${entry.timeAgo}</div>
                                <div class="memory-content">
                                    <div class="memory-tag ${entry.type}">${entry.type.toUpperCase()}</div>
                                    <div class="memory-title">${escapeHtml(entry.title)}</div>
                                    <div class="memory-details">${escapeHtml(entry.content.substring(0, 150))}...</div>
                                </div>
                            </div>
                        `).join('');
                    }
                } catch (e) {
                    console.log('Recent memories: using defaults');
                }
            }

            // Memory filter buttons
            document.querySelectorAll('.memory-filters .filter-btn').forEach(btn => {
                btn.addEventListener('click', () => {
                    document.querySelectorAll('.memory-filters .filter-btn').forEach(b => b.classList.remove('active'));
                    btn.classList.add('active');
                    const filter = btn.dataset.filter;
                    filterMemoryTimeline(filter);
                });
            });

            function filterMemoryTimeline(filter) {
                const entries = document.querySelectorAll('.memory-entry');
                entries.forEach(entry => {
                    const tag = entry.querySelector('.memory-tag');
                    if (filter === 'all' || tag.classList.contains(filter)) {
                        entry.style.display = 'flex';
                    } else {
                        entry.style.display = 'none';
                    }
                });
            }

            // Memory search - REAL API
            const searchMemoryBtn = document.getElementById('search-memory-btn');
            if (searchMemoryBtn) {
                searchMemoryBtn.addEventListener('click', async () => {
                    const query = document.getElementById('memory-search').value;
                    if (!query) return;

                    addConsoleOutput(`Searching for: "${query}"...`);

                    try {
                        const response = await fetch(`${API_BASE}/api/memory/search?q=${encodeURIComponent(query)}`);
                        const data = await response.json();
                        addConsoleOutput(`Found ${data.count} memories matching "${query}"`);

                        if (data.results && data.results.length > 0) {
                            data.results.slice(0, 5).forEach((r, i) => {
                                addConsoleOutput(`  [${i+1}] ${r.file} (score: ${r.score})`);
                            });
                        }
                    } catch (e) {
                        addConsoleOutput(`Found 15 memories matching "${query}" (demo)`);
                    }
                });
            }

            // Memory console - REAL API
            const memoryConsoleInput = document.getElementById('memory-console-input');
            if (memoryConsoleInput) {
                memoryConsoleInput.addEventListener('keypress', async (e) => {
                    if (e.key === 'Enter') {
                        const command = memoryConsoleInput.value.trim();
                        if (command) {
                            await executeMemoryCommand(command);
                            memoryConsoleInput.value = '';
                        }
                    }
                });
            }

            async function executeMemoryCommand(cmd) {
                const output = document.getElementById('memory-console-output');

                // Remove cursor and add command
                output.innerHTML = output.innerHTML.replace(/<span class="console-cursor">‚ñã<\/span>/, '');
                output.innerHTML += `\n<span class="console-prompt">blackroad-memory></span> ${cmd}\n`;

                try {
                    const response = await fetch(`${API_BASE}/api/memory/command`, {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ command: cmd })
                    });
                    const data = await response.json();
                    output.innerHTML += data.output + '\n';
                } catch (e) {
                    // Fallback to local commands
                    const localCommands = {
                        'help': `Available commands:
  status  - Check memory system status
  search  - Search memories (usage: search "query")
  log     - Add a memory entry
  sync    - Force sync with all agents
  stats   - Show memory statistics`,
                        'status': `<span class="console-success">‚úì Memory System: ONLINE (demo mode)</span>
<span class="console-info">  Start server with: npm start</span>`,
                        'stats': `<span class="console-info">Memory Statistics (demo):</span>
<span class="console-result">  - Total entries: 4,127</span>
<span class="console-result">  - Codex components: 8,789</span>`
                    };

                    if (localCommands[cmd]) {
                        output.innerHTML += localCommands[cmd] + '\n';
                    } else {
                        output.innerHTML += `<span style="color: #F5A623;">Server offline. Start with: npm start</span>\n`;
                    }
                }

                output.innerHTML += `<span class="console-prompt">blackroad-memory></span> <span class="console-cursor">‚ñã</span>`;
                output.scrollTop = output.scrollHeight;
            }

            function addConsoleOutput(text) {
                const output = document.getElementById('memory-console-output');
                output.innerHTML = output.innerHTML.replace(/<span class="console-cursor">‚ñã<\/span>/, '');
                output.innerHTML += `<span class="console-info">${text}</span>\n`;
                output.innerHTML += `<span class="console-prompt">blackroad-memory></span> <span class="console-cursor">‚ñã</span>`;
                output.scrollTop = output.scrollHeight;
            }

            // ========== COLLABORATION SECTION - REAL API ==========

            // Load agents
            async function loadAgents() {
                try {
                    const response = await fetch(`${API_BASE}/api/agents`);
                    const data = await response.json();

                    if (data.agents && data.agents.length > 0) {
                        updateAgentNetwork(data.agents);
                    }

                    // Update stats
                    const statsResponse = await fetch(`${API_BASE}/api/agents/stats`);
                    const stats = await statsResponse.json();
                    updateCollabStats(stats);
                } catch (e) {
                    console.log('Agents: using defaults');
                }
            }

            function updateAgentNetwork(agents) {
                // Update network status
                const statusEl = document.querySelector('.network-status span:last-child');
                if (statusEl) {
                    statusEl.textContent = `${agents.filter(a => a.status === 'active').length} agents active`;
                }
            }

            function updateCollabStats(stats) {
                const statEls = document.querySelectorAll('.collab-stat-value');
                if (statEls.length >= 4) {
                    statEls[0].textContent = stats.active || 12;
                }
            }

            // Load tasks - REAL API
            async function loadTasks(priority = 'all') {
                try {
                    const url = priority === 'all'
                        ? `${API_BASE}/api/tasks`
                        : `${API_BASE}/api/tasks?priority=${priority}`;

                    const response = await fetch(url);
                    const data = await response.json();

                    if (data.tasks) {
                        renderTasks(data.tasks);
                    }
                } catch (e) {
                    console.log('Tasks: using defaults');
                }
            }

            function renderTasks(tasks) {
                const taskList = document.getElementById('task-list');
                taskList.innerHTML = tasks.map(task => `
                    <div class="task-card ${task.priority}" data-task-id="${task.id}">
                        <div class="task-priority">${getPriorityEmoji(task.priority)} ${task.priority.toUpperCase()}</div>
                        <div class="task-title">${escapeHtml(task.title)}</div>
                        <div class="task-meta">
                            <span class="task-skills">${task.skills || 'general'}</span>
                            ${task.status === 'claimed'
                                ? `<span class="task-claimed">Claimed by: <strong>${task.claimed_by}</strong></span>`
                                : `<span class="task-available">Available</span>`
                            }
                        </div>
                        ${task.status === 'claimed'
                            ? `<div class="task-progress"><div class="progress-bar" style="width: ${task.progress}%;"></div></div>`
                            : `<button class="btn-small" onclick="claimTask('${task.id}')">Claim Task</button>`
                        }
                    </div>
                `).join('');
            }

            function getPriorityEmoji(priority) {
                return { urgent: 'üî¥', high: 'üü†', medium: 'üü°', low: 'üü¢' }[priority] || '‚ö™';
            }

            // Claim task - REAL API
            window.claimTask = async function(taskId) {
                try {
                    await fetch(`${API_BASE}/api/tasks/${taskId}/claim`, {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ agent: 'You' })
                    });

                    // Reload tasks
                    loadTasks();

                    // Add chat notification
                    addChatMessage('Coordinator', 'üéØ', `Task claimed successfully. Good luck!`);
                } catch (e) {
                    // Fallback UI update
                    const taskCard = document.querySelector(`[data-task-id="${taskId}"]`);
                    if (taskCard) {
                        const btn = taskCard.querySelector('.btn-small');
                        if (btn) {
                            btn.textContent = 'Claimed!';
                            btn.style.background = '#00FF00';
                            btn.style.color = '#000';
                            btn.disabled = true;
                        }
                        const available = taskCard.querySelector('.task-available');
                        if (available) {
                            available.className = 'task-claimed';
                            available.innerHTML = 'Claimed by: <strong>You</strong>';
                        }
                    }
                }
            };

            // Task filter buttons
            document.querySelectorAll('.task-filters .filter-btn').forEach(btn => {
                btn.addEventListener('click', () => {
                    document.querySelectorAll('.task-filters .filter-btn').forEach(b => b.classList.remove('active'));
                    btn.classList.add('active');
                    const priority = btn.dataset.priority;
                    loadTasks(priority);
                });
            });

            // Agent chat - REAL API
            const sendChatBtn = document.getElementById('send-chat-btn');
            if (sendChatBtn) {
                sendChatBtn.addEventListener('click', sendAgentMessage);
            }

            const agentChatInput = document.getElementById('agent-chat-input');
            if (agentChatInput) {
                agentChatInput.addEventListener('keypress', (e) => {
                    if (e.key === 'Enter') sendAgentMessage();
                });
            }

            async function sendAgentMessage() {
                const input = document.getElementById('agent-chat-input');
                const message = input.value.trim();

                if (!message) return;

                // Add user message to UI immediately
                addChatMessage('You', 'üë§', message);
                input.value = '';

                try {
                    // Send to server
                    await fetch(`${API_BASE}/api/chat/send`, {
                        method: 'POST',
                        headers: { 'Content-Type': 'application/json' },
                        body: JSON.stringify({ message, sender: 'You' })
                    });

                    // Server will add agent response - poll for it
                    setTimeout(async () => {
                        try {
                            const response = await fetch(`${API_BASE}/api/chat/history`);
                            const data = await response.json();
                            const lastMsg = data.messages[data.messages.length - 1];
                            if (lastMsg && lastMsg.sender !== 'You') {
                                addChatMessage(lastMsg.sender, lastMsg.avatar, lastMsg.text);
                            }
                        } catch (e) {
                            // Fallback response
                            simulateAgentResponse();
                        }
                    }, 1200);
                } catch (e) {
                    // Fallback to simulation
                    simulateAgentResponse();
                }
            }

            function simulateAgentResponse() {
                const agents = [
                    { name: 'Apollo', avatar: '‚ö°', responses: ['Processing your request.', 'Optimizing now.'] },
                    { name: 'Aria', avatar: 'üèóÔ∏è', responses: ['Deploying changes.', 'Infrastructure updated.'] },
                    { name: 'Athena', avatar: 'üîí', responses: ['Security check passed.', 'No issues detected.'] },
                    { name: 'Atlas', avatar: 'üìä', responses: ['Metrics updated.', 'Dashboard refreshed.'] }
                ];
                const agent = agents[Math.floor(Math.random() * agents.length)];
                const response = agent.responses[Math.floor(Math.random() * agent.responses.length)];
                addChatMessage(agent.name, agent.avatar, response);
            }

            function addChatMessage(sender, avatar, text) {
                const chat = document.getElementById('agent-chat');
                const msg = document.createElement('div');
                msg.className = 'chat-message';
                msg.innerHTML = `
                    <div class="chat-avatar">${avatar}</div>
                    <div class="chat-content">
                        <div class="chat-author">${sender}</div>
                        <div class="chat-text">${escapeHtml(text)}</div>
                        <div class="chat-time">${new Date().toLocaleTimeString([], {hour: '2-digit', minute:'2-digit'})}</div>
                    </div>
                `;
                chat.appendChild(msg);
                chat.scrollTop = chat.scrollHeight;
            }

            // Initialize collaboration data when tab is shown
            document.querySelector('[data-tab="collaboration"]').addEventListener('click', () => {
                loadAgents();
                loadTasks();
            });

            // Initialize memory data when tab is shown
            document.querySelector('[data-tab="memory"]').addEventListener('click', () => {
                loadMemoryStats();
                loadRecentMemories();
            });
            // Model selector
            document.querySelectorAll('.model-option').forEach(option => {
            const selectModel = () => {
                // Remove active class from all options
                document.querySelectorAll('.model-option').forEach(o => {
                    o.classList.remove('active');
                    o.setAttribute('aria-pressed', 'false');
                });
                // Add active class to clicked option
                option.classList.add('active');
                option.setAttribute('aria-pressed', 'true');
                // Update selected model display
                const modelName = option.querySelector('.model-name').textContent;
                const display = document.getElementById('selected-model-display');
                if (display) {
                    display.textContent = modelName;
                }
            };

            option.addEventListener('click', selectModel);
            
            // Keyboard support (Enter and Space keys)
            option.addEventListener('keydown', (e) => {
                if (e.key === 'Enter' || e.key === ' ') {
                    e.preventDefault();
                    selectModel();
                }
            });
        });

        // Sliders
        document.getElementById('temperature').addEventListener('input', (e) => {
            document.getElementById('temp-value').textContent = e.target.value;
        });
        document.getElementById('max-tokens').addEventListener('input', (e) => {
            document.getElementById('tokens-value').textContent = e.target.value;
        });
        document.getElementById('top-p').addEventListener('input', (e) => {
            document.getElementById('topp-value').textContent = e.target.value;
        });

        // API Base URL - auto-detect if running on server or static
        const API_BASE = window.location.port === '3000' ? '' : 'http://localhost:3000';

        // Generate button - REAL API CALL
        document.getElementById('generate-btn').addEventListener('click', async () => {
            const prompt = document.getElementById('prompt-input').value;
            const output = document.getElementById('output');
            const btn = document.getElementById('generate-btn');

            if (!prompt.trim()) {
                output.innerHTML = '<span style="color: #FF1D6C;">‚ö†Ô∏è Please enter a prompt first.</span>';
                return;
            }

            // Get selected model
            const activeModel = document.querySelector('.model-option.active');
            const modelId = activeModel ? activeModel.dataset.model : 'claude-sonnet-4';
            const modelName = activeModel ? activeModel.querySelector('.model-name').textContent : 'Claude Sonnet 4';

            // Get parameters
            const temperature = parseFloat(document.getElementById('temperature').value);
            const maxTokens = parseInt(document.getElementById('max-tokens').value);

            btn.disabled = true;
            btn.style.opacity = '0.6';
            btn.textContent = '‚è≥ Generating...';
            output.innerHTML = '<span class="loading">‚ñã Connecting to ' + modelName + '...</span>';

            try {
                const response = await fetch(`${API_BASE}/api/ai/generate`, {
                    method: 'POST',
                    headers: { 'Content-Type': 'application/json' },
                    body: JSON.stringify({
                        prompt,
                        model: modelId,
                        temperature,
                        maxTokens
                    })
                });

                const data = await response.json();

                if (data.success) {
                    const latency = data.latency || 'N/A';
                    const simulated = data.simulated ? ' (simulated)' : '';
                    output.innerHTML = `<div style="white-space: pre-wrap;">${escapeHtml(data.response)}</div>
                    <div style="margin-top: 21px; padding-top: 13px; border-top: 1px solid rgba(255,255,255,0.1); font-size: 12px; opacity: 0.7;">
                        ü§ñ Model: ${modelName}${simulated} | ‚ö° Latency: ${latency}ms | üéØ Temp: ${temperature}
                    </div>`;
                } else {
                    output.innerHTML = `<span style="color: #FF1D6C;">‚ùå Error: ${data.error || 'Unknown error'}</span>`;
                }
            } catch (error) {
                // Fallback to simulation if server not running
                output.innerHTML = `<span style="color: #F5A623;">‚ö†Ô∏è Server not running. Start with: npm start</span>
                <div style="margin-top: 13px; opacity: 0.7;">Falling back to demo mode...</div>`;

                setTimeout(() => {
                    output.innerHTML = `# Demo Response from ${modelName}

Based on your prompt: "${prompt.substring(0, 50)}..."

This is a demo response. To get real AI responses:

1. Run: cd ~/blackroad-ai-platform && npm install
2. Run: npm start
3. Refresh this page

The server connects to:
- Claude API (if ANTHROPIC_API_KEY is set)
- Ollama (for local models)
- BlackRoad Memory System
- Agent Registry

**Demo Mode** ü§ñ | Start the server for real AI!`;
                }, 1000);
            }

            btn.disabled = false;
            btn.style.opacity = '1';
            btn.textContent = '‚ú® Generate AI Response';
        });

        // Helper to escape HTML
        function escapeHtml(text) {
            const div = document.createElement('div');
            div.textContent = text;
            return div.innerHTML;
        }

        // Smooth scroll
        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
            anchor.addEventListener('click', function (e) {
                e.preventDefault();
                const target = document.querySelector(this.getAttribute('href'));
                if (target) target.scrollIntoView({ behavior: 'smooth' });
            });
        });

        // ========== SETTINGS FUNCTIONS ==========
        
        function saveApiKey(provider) {
            const input = document.getElementById(`${provider}-key`);
            const key = input.value.trim();
            
            if (!key) {
                alert('Please enter an API key');
                return;
            }
            
            // Save to localStorage (in production, this should be encrypted and server-side)
            localStorage.setItem(`${provider}_api_key`, key);
            alert(`‚úÖ ${provider.charAt(0).toUpperCase() + provider.slice(1)} API key saved!`);
            
            // Clear input for security
            input.value = '';
        }

        function setTheme(theme) {
            document.querySelectorAll('.theme-option').forEach(opt => opt.classList.remove('active'));
            event.target.closest('.theme-option').classList.add('active');
            localStorage.setItem('theme', theme);
            
            // Apply theme (simplified demo)
            console.log(`Theme changed to: ${theme}`);
            alert(`‚úÖ Theme "${theme}" applied!`);
        }

        function exportSettings() {
            const settings = {
                theme: localStorage.getItem('theme') || 'dark-purple',
                defaultModel: document.getElementById('default-model').value,
                autoSave: document.getElementById('auto-save').checked,
                streamResponses: document.getElementById('stream-responses').checked,
                fontSize: document.getElementById('font-size').value,
                exportDate: new Date().toISOString()
            };
            
            const blob = new Blob([JSON.stringify(settings, null, 2)], { type: 'application/json' });
            const url = URL.createObjectURL(blob);
            const a = document.createElement('a');
            a.href = url;
            a.download = `blackroad-settings-${Date.now()}.json`;
            a.click();
            URL.revokeObjectURL(url);
        }

        function importSettings(input) {
            const file = input.files[0];
            if (!file) return;
            
            const reader = new FileReader();
            reader.onload = (e) => {
                try {
                    const settings = JSON.parse(e.target.result);
                    
                    if (settings.theme) localStorage.setItem('theme', settings.theme);
                    if (settings.defaultModel) document.getElementById('default-model').value = settings.defaultModel;
                    if (typeof settings.autoSave !== 'undefined') document.getElementById('auto-save').checked = settings.autoSave;
                    if (typeof settings.streamResponses !== 'undefined') document.getElementById('stream-responses').checked = settings.streamResponses;
                    if (settings.fontSize) document.getElementById('font-size').value = settings.fontSize;
                    
                    alert('‚úÖ Settings imported successfully!');
                } catch (err) {
                    alert('‚ùå Error importing settings: Invalid file format');
                }
            };
            reader.readAsText(file);
        }

        function clearAllData() {
            if (confirm('‚ö†Ô∏è This will clear all your data including API keys, settings, and history. Are you sure?')) {
                localStorage.clear();
                alert('‚úÖ All data cleared!');
                location.reload();
            }
        }

        // ========== HISTORY FUNCTIONS ==========
        
        function filterHistory() {
            const searchTerm = document.getElementById('history-search').value.toLowerCase();
            const modelFilter = document.getElementById('history-model-filter').value;
            const timeFilter = document.getElementById('history-time-filter').value;
            
            document.querySelectorAll('.history-item').forEach(item => {
                const text = item.textContent.toLowerCase();
                const model = item.dataset.model;
                const time = item.dataset.time;
                
                const matchesSearch = !searchTerm || text.includes(searchTerm);
                const matchesModel = modelFilter === 'all' || model.includes(modelFilter);
                const matchesTime = timeFilter === 'all' || time === timeFilter;
                
                item.style.display = matchesSearch && matchesModel && matchesTime ? 'flex' : 'none';
            });
        }

        function viewConversation(id) {
            alert(`üìñ Opening conversation: ${id}\n\nThis would load the full conversation in a modal or new page.`);
        }

        function shareConversation(id) {
            const url = `${window.location.origin}/conversation/${id}`;
            navigator.clipboard.writeText(url).then(() => {
                alert(`‚úÖ Share link copied to clipboard!\n\n${url}`);
            });
        }

        function deleteConversation(id) {
            if (confirm('‚ö†Ô∏è Are you sure you want to delete this conversation?')) {
                alert(`‚úÖ Conversation ${id} deleted!`);
                event.target.closest('.history-item').remove();
            }
        }

        function usePrompt(promptId) {
            const prompts = {
                'code-review': 'Review this code for bugs, performance issues, and best practices:\n\n',
                'email': 'Write a professional email about:\n\n',
                'brainstorm': 'Generate 10 creative ideas for:\n\n'
            };
            
            const prompt = prompts[promptId];
            if (prompt) {
                // Switch to AI tab and fill prompt
                const aiTab = document.querySelector('[data-tab="ai"]');
                if (aiTab) aiTab.click();
                
                setTimeout(() => {
                    const promptInput = document.getElementById('prompt-input');
                    if (promptInput) {
                        promptInput.value = prompt;
                        promptInput.focus();
                    }
                }, 300);
            }
        }

        // Animate cards
        const observer = new IntersectionObserver((entries) => {
            entries.forEach(entry => {
                if (entry.isIntersecting) {
                    entry.target.style.opacity = '1';
                    entry.target.style.transform = 'translateY(0)';
                }
            });
        }, { threshold: 0.1, rootMargin: '0px 0px -100px 0px' });

        document.querySelectorAll('.feature-card, .model-card').forEach(card => {
            card.style.opacity = '0';
            card.style.transform = 'translateY(30px)';
            card.style.transition = 'opacity 0.6s ease, transform 0.6s ease';
            observer.observe(card);
        });
        }); // End DOMContentLoaded
    </script>
</body>
</html>
